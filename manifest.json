{
    "manifest_version": 3,
    "name": "Website Clusterer",
    "version": "3.0",
    "permissions": [
        "storage",
        "history",
        "alarms",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}
